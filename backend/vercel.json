{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/test", "dest": "/api/test" },
    { "src": "/health", "dest": "/api/health" },
    { "src": "/", "dest": "/api/" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ]
}
